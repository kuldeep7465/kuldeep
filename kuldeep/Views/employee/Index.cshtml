
@{
    ViewBag.Title = "Index";
}
@model List<kuldeep.db_content.my_table>
    <table class="table table-hover">
        <tr>
            <th>id</th>
            <th>name</th>
            <th>email</th>
            <th>EDIT</th>
            <th>DELETE</th>
        </tr>
        @{ 
            foreach (var item in Model)
            {
                <tr>
                    <td>@item.id</td>
                    <td>@item.name</td>
                    <td>@item.email</td>
                    <td><a href="@Url.Action("delete","employee",new { item.id})" class="btn btn-danger">DELETE</a></td>
                    <td><a href="@Url.Action("edit","employee",new { item.id})" class="btn btn-danger">EDIT</a></td>
                </tr>
                }
            }
    </table>
    <h2>Index</h2>

